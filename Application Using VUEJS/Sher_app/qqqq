  for filename in glob.glob(os.path.join(path, '*.txt')):
        hasher = hashlib.md5()
        with open(filename, 'rb') as afile:
            buf = afile.read()
            hasher.update(buf)
            hash = hasher.hexdigest()

            if File.objects.filter(Hashvalue=hash).exists():
                print("Existing File")